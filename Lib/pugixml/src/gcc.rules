# -*- Makefile -*-
G++         = g++
GCC         = gcc
COMMINC		= -I. -I$(HOME)/luajit/include
LNKFLAG     = -shared
COMPWARN    = -Wall
CMPFLAG     = -c -g  -fPIC -pthread $(COMPWARN) $(COMMINC)  -D_UNIX95_
SYTMLIB 	= -lc -lm -ldl -lpthread  -Wl,-rpath -lrt
PLIBDIR     = -L$(HOME)/lib -L$(HOME)/luajit/lib

AR          = ar
ARFLAGS     = -rcu
RANLIB      = ranlib
RANLIBFLAGS =


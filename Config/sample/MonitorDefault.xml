<?xml version="1.0" encoding="UTF-8"?>
<Configure>
    <_STRUCTURE>
        <MonitorList>
            <Monitor>
                <Base>
                    <Alias>Monitor0001</Alias>
                    <Token>MTR.00.CTR</Token>
                    <SVCLimits>4</SVCLimits>
                </Base>
                <LowLoger>
                    <Level>0</Level>
                </LowLoger>
                <AppLoger>
                    <_SCROLLTYPE>0</_SCROLLTYPE>
                    <_UNIONSELECTOR>
                        <DATEMODE>
                            <NamePattern>~/log/$(date)/P$(MC)_$(TC)_$(seq).log</NamePattern>
                        </DATEMODE>
                        <APPMODE>
                            <NamePattern></NamePattern>
                        </APPMODE>
                        <CUSTOMMODE>
                            <NamePattern></NamePattern>
                        </CUSTOMMODE>
                    </_UNIONSELECTOR>
                </AppLoger>
                <SVC>
                    <Threads>8</Threads>
                    <IsFlush>1</IsFlush>
                    <DBConnection>
                        <_SCROLLTYPE>4</_SCROLLTYPE>
                        <_UNIONSELECTOR>
                            <ORACLE>
                              <Name>orcl</Name>
                              <User>system</User>
                              <Passwd>oracle</Passwd>
                            </ORACLE>
                            <DB2>
                                <Name></Name>
                                <User></User>
                                <Passwd></Passwd>
                            </DB2>
                            <SQLITE>
                                <DBFile>SqliteDB</DBFile>
                            </SQLITE>
                         </_UNIONSELECTOR>
                    </DBConnection>
                </SVC>
                <LSRLists>
                    <LSRList0>
                        <LSR>
                            <Alias>Listener0000</Alias>
                            <ImplDllName>demo.app</ImplDllName>
                            <Threads>8</Threads>
                            <ThreadStackSize>4194304</ThreadStackSize>
                            <Protocol>
                                <_SCROLLTYPE>0</_SCROLLTYPE>
                                <_UNIONSELECTOR>
                                    <LSRSTREAM>
                                        <Address>
                                            <_SCROLLTYPE>0</_SCROLLTYPE>
                                            <_UNIONSELECTOR>
                                                <UNIX>
                                                    <Token></Token>
                                                </UNIX>
                                                <TCP>
                                                    <IP>127.0.0.1</IP>
                                                    <Port>9000</Port>
                                                </TCP>
                                            </_UNIONSELECTOR>
                                        </Address>
                                        <RecvTimeOut>30</RecvTimeOut>
                                        <ExchangeTMO>30</ExchangeTMO>
                                        <PTCDllName>Demo.PTC</PTCDllName>
                                    </LSRSTREAM>
                                </_UNIONSELECTOR>
                            </Protocol>
                        </LSR>
                    </LSRList0>
                    <LSRList1>
                        <LSR>
                            <Alias>Listener0001</Alias>
                            <ImplDllName>natp.app</ImplDllName>
                            <Threads>8</Threads>
                            <ThreadStackSize>4194304</ThreadStackSize>
                            <Protocol>
                                <_SCROLLTYPE>0</_SCROLLTYPE>
                                <_UNIONSELECTOR>
                                    <LSRSTREAM>
                                        <Address>
                                            <_SCROLLTYPE>0</_SCROLLTYPE>
                                            <_UNIONSELECTOR>
                                                <UNIX>
                                                    <Token></Token>
                                                </UNIX>
                                                <TCP>
                                                    <IP>127.0.0.1</IP>
                                                    <Port>9001</Port>
                                                </TCP>
                                            </_UNIONSELECTOR>
                                        </Address>
                                        <RecvTimeOut>30</RecvTimeOut>
                                        <ExchangeTMO>30</ExchangeTMO>
                                        <PTCDllName>Demo.PTC</PTCDllName>
                                    </LSRSTREAM>
                                </_UNIONSELECTOR>
                            </Protocol>
                        </LSR>
                    </LSRList1>
                </LSRLists>
                <CNRLists>
                    <CNRList0>
                        <CNR>
                            <Alias>__TMO__</Alias>
                            <PlugIn>
                                <_SCROLLTYPE>1</_SCROLLTYPE>
                                <_UNIONSELECTOR>
                                    <ECHO>
                                        <ImplDllName>EchoCNR.so</ImplDllName>
                                    </ECHO>
                                    <TIMEOUT>
                                        <ImplDllName>TimeOutCNR.so</ImplDllName>
                                    </TIMEOUT>
                                    <SQUARE>
                                        <ImplDllName>SquareCNR.so</ImplDllName>
                                    </SQUARE>
                                    <STATE>
                                        <ImplDllName>StatusCNR.so</ImplDllName>
                                    </STATE>
                                </_UNIONSELECTOR>
                            </PlugIn>
                        </CNR>
                    </CNRList0>
                    <CNRList1>
                        <CNR>
                            <Alias>status</Alias>
                            <PlugIn>
                                <_SCROLLTYPE>3</_SCROLLTYPE>
                                <_UNIONSELECTOR>
                                    <ECHO>
                                        <ImplDllName>EchoCNR.so</ImplDllName>
                                    </ECHO>
                                    <TIMEOUT>
                                        <ImplDllName>TimeOutCNR.so</ImplDllName>
                                    </TIMEOUT>
                                    <SQUARE>
                                        <ImplDllName>SquareCNR.so</ImplDllName>
                                    </SQUARE>
                                    <STATE>
                                        <ImplDllName>StatusCNR.so</ImplDllName>
                                    </STATE>
                                </_UNIONSELECTOR>
                            </PlugIn>
                        </CNR>
                    </CNRList1>
                </CNRLists>
            </Monitor>
        </MonitorList>
    </_STRUCTURE>
</Configure>

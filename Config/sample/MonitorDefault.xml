<?xml version="1.0" encoding="UTF-8"?>
<Configure>
    <_STRUCTURE>
        <MonitorList>
            <Monitor>
                <LuaEnvLists>
                    <LuaEnvList0>
                        <LuaEnv>
                            <Path>~/lib/lua/?.lua</Path>
                        </LuaEnv>
                    </LuaEnvList0>
                    <LuaEnvList1>
                        <LuaEnv>
                            <Path>~/lib/lua/?.so</Path>
                        </LuaEnv>
                    </LuaEnvList1>
                </LuaEnvLists>
                <Base>
                    <Alias>Monitor0001</Alias>
                    <AMQToken>AMQToken.chl</AMQToken>
                    <RTToken>RouterToken.db</RTToken>
                    <Token>MTR.00.CTR</Token>
                    <SVCLimits>4</SVCLimits>
                </Base>
                <LowLoger>
                    <Level>0</Level>
                </LowLoger>
                <AppLoger>
                    <_SCROLLTYPE>0</_SCROLLTYPE>
                    <_UNIONSELECTOR>
                        <DATEMODE>
                            <NamePattern>~/log/$(date)/P$(MC)_$(TC)_$(seq).log</NamePattern>
                        </DATEMODE>
                        <APPMODE>
                            <NamePattern></NamePattern>
                        </APPMODE>
                        <CUSTOMMODE>
                            <NamePattern></NamePattern>
                        </CUSTOMMODE>
                    </_UNIONSELECTOR>
                </AppLoger>
                <SVC>
                    <Threads>8</Threads>
                    <IsFlush>1</IsFlush>
                    <DBConnection>
                        <_SCROLLTYPE>4</_SCROLLTYPE>
                        <_UNIONSELECTOR>
                            <ORACLE>
                              <Name>orcl</Name>
                              <User>system</User>
                              <Passwd>oracle</Passwd>
                            </ORACLE>
                            <DB2>
                                <Name></Name>
                                <User></User>
                                <Passwd></Passwd>
                            </DB2>
                            <SQLITE>
                                <DBFile>SqliteDB</DBFile>
                            </SQLITE>
                         </_UNIONSELECTOR>
                    </DBConnection>
                </SVC>
                <CTR>
                    <Address>
                        <IP>0.0.0.0</IP>
                        <Port>5000</Port>
                    </Address>
                </CTR>
                <LSRLists>
                    <LSRList0>
                        <LSR>
                            <Alias>Listener0000</Alias>
                            <ImplDllName>demo.app</ImplDllName>
                            <Threads>8</Threads>
                            <ThreadStackSize>4194304</ThreadStackSize>
                            <Protocol>
                                <_SCROLLTYPE>0</_SCROLLTYPE>
                                <_UNIONSELECTOR>
                                    <LSRSTREAM>
                                        <Address>
                                            <_SCROLLTYPE>0</_SCROLLTYPE>
                                            <_UNIONSELECTOR>
                                                <UNIX>
                                                    <Token></Token>
                                                </UNIX>
                                                <TCP>
                                                    <IP>127.0.0.1</IP>
                                                    <Port>9000</Port>
                                                </TCP>
                                            </_UNIONSELECTOR>
                                        </Address>
                                        <RecvTimeOut>30</RecvTimeOut>
                                        <ExchangeTMO>30</ExchangeTMO>
                                        <PTCDllName>Demo.PTC</PTCDllName>
                                    </LSRSTREAM>
                                </_UNIONSELECTOR>
                            </Protocol>
                        </LSR>
                    </LSRList0>
                    <LSRList1>
                        <LSR>
                            <Alias>Listener0001</Alias>
                            <ImplDllName>natp.app</ImplDllName>
                            <Threads>8</Threads>
                            <ThreadStackSize>4194304</ThreadStackSize>
                            <Protocol>
                                <_SCROLLTYPE>0</_SCROLLTYPE>
                                <_UNIONSELECTOR>
                                    <LSRSTREAM>
                                        <Address>
                                            <_SCROLLTYPE>0</_SCROLLTYPE>
                                            <_UNIONSELECTOR>
                                                <UNIX>
                                                    <Token></Token>
                                                </UNIX>
                                                <TCP>
                                                    <IP>127.0.0.1</IP>
                                                    <Port>9001</Port>
                                                </TCP>
                                            </_UNIONSELECTOR>
                                        </Address>
                                        <RecvTimeOut>30</RecvTimeOut>
                                        <ExchangeTMO>30</ExchangeTMO>
                                        <PTCDllName>Demo.PTC</PTCDllName>
                                    </LSRSTREAM>
                                </_UNIONSELECTOR>
                            </Protocol>
                        </LSR>
                    </LSRList1>
                </LSRLists>
            </Monitor>
        </MonitorList>
    </_STRUCTURE>
</Configure>
